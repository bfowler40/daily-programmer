oom([[6, 8], [5, 8], [8, 9], [5, 7], [4, 7]]);\n\n\tconsole.log(scenario1.hours());\n\tconsole.log(scenario2.hours());\n\tconsole.log(scenario3.hours());\n}\n"},"lineCount":null}},"hash":"17ca3f774a60ffbff1eb90e92c9504d5","cacheData":{"env":{}},"id":20}