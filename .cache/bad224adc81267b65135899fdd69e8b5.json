64-create-a-dice-roller.ts","name":null,"original":{"line":69,"column":24},"generated":{"line":92,"column":17}},{"source":"challenges/364-create-a-dice-roller.ts","name":null,"original":{"line":69,"column":28},"generated":{"line":92,"column":21}},{"source":"challenges/364-create-a-dice-roller.ts","name":null,"original":{"line":69,"column":24},"generated":{"line":92,"column":31}},{"source":"challenges/364-create-a-dice-roller.ts","name":null,"original":{"line":69,"column":39},"generated":{"line":92,"column":32}},{"source":"challenges/364-create-a-dice-roller.ts","name":null,"original":{"line":69,"column":24},"generated":{"line":92,"column":39}},{"source":"challenges/364-create-a-dice-roller.ts","name":null,"original":{"line":69,"column":0},"generated":{"line":92,"column":40}},{"source":"challenges/364-create-a-dice-roller.ts","name":null,"original":{"line":70,"column":0},"generated":{"line":93,"column":0}},{"source":"challenges/364-create-a-dice-roller.ts","name":null,"original":{"line":70,"column":11},"generated":{"line":93,"column":11}},{"source":"challenges/364-create-a-dice-roller.ts","name":null,"original":{"line":70,"column":0},"generated":{"line":93,"column":18}},{"source":"challenges/364-create-a-dice-roller.ts","name":null,"original":{"line":72,"column":0},"generated":{"line":94,"column":0}},{"source":"challenges/364-create-a-dice-roller.ts","name":null,"original":{"line":72,"column":0},"generated":{"line":94,"column":8}},{"source":"challenges/364-create-a-dice-roller.ts","name":null,"original":{"line":72,"column":0},"generated":{"line":94,"column":15}},{"source":"challenges/364-create-a-dice-roller.ts","name":null,"original":{"line":72,"column":15},"generated":{"line":94,"column":18}},{"source":"challenges/364-create-a-dice-roller.ts","name":null,"original":{"line":72,"column":0},"generated":{"line":94,"column":28}}],"sources":{"challenges/364-create-a-dice-roller.ts":"/**\n * Challenge #364 - Create a Dice Roller\n * https://www.reddit.com/r/dailyprogrammer/comments/8s0cy1/20180618_challenge_364_easy_create_a_dice_roller/\n */\n\nconst diceBag = ['5d12', '6d4', '1d2', '1d8', '3d6', '4d20', '100d100'];\n\nclass DiceRoller {\n\tprotected dice: string[];\n\n\t/**\n\t * Creare the dice roller\n\t * @param {string[]} dice\n\t */\n\tconstructor(dice: string[]) {\n\t\tthis.dice = dice;\n\t}\n\n\t/**\n\t * Roll dice on all inputs\n\t */\n\tpublic rollAll(): void {\n\t\tthis.dice.forEach((die: string): void => this._roll(die));\n\t}\n\n\t/**\n\t * Return a random number\n\t * @param {number} max\n\t * @return {number} The random number\n\t */\n\tprivate _random(max: number): number {\n\t\treturn Math.floor(Math.random() * max) + 1;\n\t}\n\n\t/**\n\t * Accumulate the results and log to console\n\t * @param {number[]} rolls\n\t */\n\tprivate _result(rolls: number[]): void {\n\t\tconst rollTotal: number = rolls.reduce(\n\t\t\t(a: number, b: number): number => a + b,\n\t\t\t0\n\t\t);\n\t\tconst rollResults: string = rolls.join(', ');\n\n\t\tconsole.log(`${rollTotal}: [${rollResults}]`);\n\t}\n\n\t/**\n\t * Roll a die\n\t * @param {string} die\n\t */\n\tprivate _roll(die: string): void {\n\t\tconst [dice, sides] = die.split('d');\n\t\tconst rolls: number[] = [];\n\t\tlet index: number = 0;\n\n\t\tfor (index; index < parseInt(dice, 10); index++) {\n\t\t\tconst result: number = this._random(parseInt(sides, 10));\n\n\t\t\trolls.push(result);\n\t\t}\n\n\t\tconsole.log(`Dice: ${dice} | Sides ${sides}`);\n\t\tthis._result(rolls);\n\t}\n}\n\nconst diceRoller: any = new DiceRoller(diceBag);\ndiceRoller.rollAll();\n\nexport default diceRoller;\n"},"lineCount":null}},"hash":"5326757ad4606f67e9acf7da56f1ab4d","cacheData":{"env":{}},"id":11}